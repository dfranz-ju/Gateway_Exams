		  <exercises>
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(x) = x^4 + \dfrac{2}{x^2} - 20\pi + \dfrac{5\sqrt{x}}{3}</m>
					  </p>
			  	
				  </statement>
			  
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(x) \amp = \frac{d}{dx}(x^4) + 2\frac{d}{dx}\left(\frac{1}{x^2}\right) - \frac{d}{dx}(20\pi) + \frac{5}{3}\frac{d}{dx}(\sqrt{x})
							  </mrow>
							  <mrow>
								  \amp = 4x^3 + 2\frac{d}{dx}(x^{-2}) - 0 + \frac{5}{3}\cdot \frac{1}{2\sqrt{x}}
							  </mrow>
							  <mrow>
								  \amp = 4x^3 - 4x^{-1} + \frac{5}{6\sqrt{x}}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(x) = \dfrac{3x-\cos(x)}{x+1}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(x) \amp = \frac{(x+1)\cdot\frac{d}{dx}\left(3x-\cos(x)\right) - \frac{d}{dx}\left(x+1\right) \cdot (3x-\cos(x)) }{(x+1)^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{(x+1)\cdot(3+\sin(x)) - (3x-\cos(x))}{(x+1)^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>r(t) = \ln(\sin(t^4)+2t))</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  r'(t) \amp = \frac{1}{\sin(t^4)+2t} \cdot \frac{d}{dt}\left(\sin(t^4)+2t\right)
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{\sin(t^4)+2t} \cdot (\cos(t^4) \cdot 4t^3 + 2t)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(z) = \arctan(3z^2+1) - 5z^3</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(z) \amp = \frac{1}{1+(3z^2+1)^2}\cdot \frac{d}{dz}\left(3z^2+1\right) - 5\frac{d}{dz}(z^3)
							  </mrow>
							  <mrow>
								  \amp = \frac{6z}{1+(3z^2+1)^2} - 15z^2
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(t) = \dfrac{e^{\sin(t)}}{\ln(t)}</m>
					  </p>
			  	
				  </statement>
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(t) \amp = \frac{e^{\sin(t)} \cdot \frac{d}{dt} \left(\ln(t)\right) - \frac{d}{dt} \left(e^{\sin(t)}\right) \cdot \ln(t)}{(\ln(t))^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{e^{\sin(t)} \cdot \frac{1}{t} - e^{\sin(t)} \cos(t) \ln(t)}{(\ln(t))^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(x) = (x^4-4x)^{12} + \sqrt{e}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(x) \amp = 12(x^4-4x)^{11} \cdot \frac{d}{dx}\left(x^4-4x\right) + 0
							  </mrow>
							  <mrow>
								  \amp = 12(x^4-4x)^{11} \cdot (4x^3-4)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(t) = e^t \cos(2t)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(t) \amp = e^t \cdot \frac{d}{dt}\left(\cos(2t)\right) + \cos(2t) \cdot \frac{d}{dt}\left(e^t\right)
							  </mrow>
							  <mrow>
								  \amp = (e^t)(-\sin(2t)\cdot 2) + \cos(2t)e^{t}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>p(y) =2^e + 2\tan(ye^y)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  p'(y) \amp = 0 + 2\sec^2(ye^y) \cdot\frac{d}{dy}(ye^y)
							  </mrow>
							  <mrow>
								  \amp = 2\sec^2(ye^y) \cdot \left(\frac{d}{dy}(y) \cdot e^y + y \cdot \frac{d}{dy}(e^y)\right)
							  </mrow>
							  <mrow>
								  \amp = 2\sec^2(ye^y) \cdot(e^y + y e^y)
							  </mrow>
						  </md>
					  </p>
					  
					  
				  
				  </solution>
			  </exercise>
			 
			  
		  </exercises>

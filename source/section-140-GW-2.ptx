		  <exercises>
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(x) = x^{16} + 2x + \dfrac{1}{x^4}-8e^2</m>
					  </p>
			  	
				  </statement>
			  
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(x) \amp = \frac{d}{dx}(x^{16}) +2\frac{d}{dx}(x) + \frac{d}{dx}(x^{-4}) - \frac{d}{dx}(8e^2)
							  </mrow>
							  <mrow>
								  \amp = 16x^{15} + 2 -4x^{-5} - 0
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(x) = (3x+\sin(x))e^{2x}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(x) \amp = \frac{d}{dx}(3x+\sin(x))\cdot e^{2x} + (3x+\sin(x))\cdot\frac{d}{dx}(e^{2x})
							  </mrow>
							  <mrow>
								  \amp = (3 + \cos(x))\cdot e^{2x} + (3x+\sin(x)) \cdot e^{2x} \cdot 2
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>r(t) = \dfrac{2\tan(t)}{t-\cos(t)}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  r'(t) \amp = \frac{ \frac{d}{dt}(2\tan(t)) \cdot (t-\cos(t)) - 2\tan(t)\cdot\frac{d}{dt}(t-\cos(t))}{(t-\cos(t))^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{ 2\sec^2(t)\cdot (t-\cos(t)) - 2\tan(t)\cdot (1+\sin(t))}{(t-\cos(t))^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(t) = \sqrt{\arctan(t^2)} + \pi^e</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(t) \amp = \frac{1}{2\sqrt{\arctan(t^2)}} \cdot \frac{d}{dt}(\arctan(t^2)) + 0
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{2\sqrt{\arctan(t^2)}} \cdot \frac{1}{1+(t^2)^2} \cdot \frac{d}{dt} (t^2)
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{2\sqrt{\arctan(t^2)}} \cdot \frac{1}{1+(t^2)^2} \cdot 2t
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>p(y) = e^{\sin(y)} + \ln(1+ y^2)</m>
					  </p>
			  	
				  </statement>
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  p'(y) \amp = e^{\sin(y)} \cdot \frac{d}{dy}(\sin(y)) + \frac{1}{1+y^2} \cdot \frac{d}{dy}(1+y^2)
							  </mrow>
							  <mrow>
								  \amp = e^{\sin(y)} \cdot \cos(y) + \frac{1}{1+y^2} \cdot 2y
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(x) = 5\sqrt{x} -  (e^x+12x)^{12}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(x) \amp = 5 \cdot \frac{1}{2\sqrt{x}} - 12(e^x + 12x)^{11}\cdot \frac{d}{dx}(e^x + 12x)
							  </mrow>
							  <mrow>
								  \amp = \frac{5}{2\sqrt{x}} - 12(e^x + 12x)^{11} \cdot(e^x+12)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(\theta) =\cos(\theta^2e^{\theta})</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(\theta) \amp = -\sin(\theta^2 e^{\theta}) \cdot \frac{d}{d\theta} (\theta^2 e^{\theta})
							  </mrow>
							  <mrow>
								  \amp = -\sin(\theta^2 e^{\theta}) \cdot \left(\frac{d}{d\theta}(\theta^2)\cdot e^{\theta} + \theta^2 \cdot \frac{d}{d\theta}(e^{\theta})\right)
							  </mrow>
							  <mrow>
								  \amp =-\sin(\theta^2 e^{\theta}) \cdot (2\theta e^{\theta} + \theta^2 e^{\theta})
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(z) = \dfrac{(1 + z^4)^3}{\ln(z)}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(z) \amp = \frac{\frac{d}{dz}((1+z^4)^3)\cdot \ln(z) - (1+z^4)^3 \cdot \frac{d}{dz}(\ln(z))}{(\ln(z))^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{3(1+z^4)^2 \cdot 4z^3 \cdot \ln(z) - (1+z^4)^3 \cdot \frac{1}{z}}{(\ln(z))^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			 
			  

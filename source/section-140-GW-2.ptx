		  <exercises>
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(x) = x^{16} + 2x + \dfrac{1}{x^4}-8e^2</m>
					  </p>
			  	
				  </statement>
			  
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(x) \amp = \frac{d}{dx}\left[ x^{16} +2x +x^{-4} -8e^2\right]
							  </mrow>
							  <mrow>
								  \amp = 16x^{15} + 2 -4x^{-5}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>y = (3x+\sin(x))e^{2x}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  \frac{dy}{dx} \amp = (3x+\sin(x))\frac{d}{dx}[e^{2x}] + e^{2x} \frac{d}{dx}[3x+\sin(x)]
							  </mrow>
							  <mrow>
								  \amp = (3x+\sin(x))\cdot e^{2x} \cdot 2 + e^{2x}(3+\cos(x))
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>r(x) = \ln(\cos(x)) + \ln(e^x)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  r'(x) \amp = \frac{d}{dx}[\ln(\cos(x)) + x]
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{\cos(x)}\cdot \frac{d}{dx}[\cos(x)]+1
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{\cos(x)} \cdot(-\sin(x))+1
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(t) = \arctan(t^2)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(t) \amp = \frac{1}{1+(t^2)^2} \cdot \frac{d}{dt}[t^2]
							  </mrow>
							  <mrow>
								  \amp = \frac{2t}{1+t^4}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(t) = \dfrac{t^2+2}{\sqrt{t}}</m>
					  </p>
			  	
				  </statement>
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(t) \amp = \frac{d}{dt} \left[\frac{t^2}{\sqrt{t}} + \frac{2}{\sqrt{t}} \right]
							  </mrow>
							  <mrow>
								  \amp = \frac{d}{dt} \left[ t^{3/2} + 2t^{-1/2}\right]
							  </mrow>
							  <mrow>
								  \amp = \frac{3}{2}t^{1/2} + 2 \cdot \frac{-1}{2}t^{-3/2}
							  </mrow>
							  <mrow>
								  \amp = \frac{3}{2}t^{1/2} -t^{-3/2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(x) = (e^x+12x)^{12}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(x) \amp = 12(e^x + 12x)^{11}\cdot \frac{d}{dx} [e^x + 12x]
							  </mrow>
							  <mrow>
								  \amp = 12(e^x + 12x)^{11} \cdot(e^x+12)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(t) = \cos^2(3t+5)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(t) \amp = \frac{d}{dt}[ (\cos(3t+5))^2]
							  </mrow>
							  <mrow>
								  \amp = 2\cos(3t+5) \cdot \frac{d}{dt}[ \cos(3t+5)]
							  </mrow>
							  <mrow>
								  \amp = 2\cos(3t+5) \cdot (-\sin(3t+5))\cdot 3
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(x) = 3^x \tan(x)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(x) \amp = 3^x \cdot \frac{d}{dx}[\tan(x)] + \tan(x) \cdot \frac{d}{dx}[3^x]
							  </mrow>
							  <mrow>
								  \amp = 3^x \cdot \sec^2(x) + \tan(x) \cdot \ln(3) \cdot 3^x
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			 
			  
		  </exercises>

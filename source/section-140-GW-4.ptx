		  <exercises>
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(x)=2x^3-\dfrac{2}{x^3}+\dfrac{3}{e^2}</m>
					  </p>
			  	
				  </statement>
			  
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(x) \amp = 2\frac{d}{dx}(x^3) - 2\frac{d}{dx}\left(\frac{1}{x^3}\right) + \frac{d}{dx}\left( \frac{3}{e^2}\right)
							  </mrow>
							  <mrow>
								  \amp = 6x^2 -2\frac{d}{dx} (x^{-3}) + 0
							  </mrow>
							  <mrow>
								  \amp = 6x^2 + 6x^{-4}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m> g(y)=\sqrt{\sin(y^2+2y+3)}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(y) \amp = \frac{1}{2\sqrt{y^2+2y+3}} \cdot \frac{d}{dy}(y^2+2y+3) 
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{2\sqrt{y^2+2y+3}} \cdot (2y+2) \amp
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>r(z)=e^z \tan(2z)-\dfrac{e^3}{3}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  r'(z) \amp = \frac{d}{dz}(e^z) \cdot \tan(2z) + e^z \cdot \frac{d}{dz}(\tan(2z)) - 0 
							  </mrow>
							  <mrow>
								  \amp = e^z\tan(2z) + e^z \sec^2(2z) \cdot 2 
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m> h(t)=\dfrac{2+\cos(t)}{\arctan(t+1)}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(t) \amp = \frac{\frac{d}{dt}(2+\cos(t)) \cdot \arctan(t+1) - (2+\cos(t)) \cdot \frac{d}{dt}(\arctan(t+1))}{(t+1)^2} 
							  </mrow>
							  <mrow>
								  \amp = \frac{-\sin(t)\cdot \arctan(t+1) - (2+\cos(t)) \cdot \frac{1}{1+(t+1)^2}}{(t+1)^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>m(\theta)=\left(3\pi+4\sin(\theta)\right)^5</m>
					  </p>
			  	
				  </statement>
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  m'(\theta) \amp = 5(3\pi+4\sin(\theta))^4 \cdot \frac{d}{d\theta} (3\pi + 4\sin(\theta)) 
							  </mrow>
							  <mrow>
								  \amp = 5(3\pi+4\sin(\theta))^4 \cdot 4\cos(\theta)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>v(n)=-\tan\big(\sin(n)\cos(n)\big)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  v'(n) \amp = -\sec^2(\sin(n)\cos(n))\cdot \frac{d}{dn}(\sin(n)\cos(n))
							  </mrow>
							  <mrow>
							  \amp = -\sec^2(\sin(n)\cos(n))\cdot \left(\frac{d}{dn}(\sin(n))\cdot \cos(n) + \sin(n)\cdot \frac{d}{dn}(\cos(n))\right) 
							  </mrow>
							  <mrow>
							  \amp = -\sec^2(\sin(n)\cos(n))\cdot (\cos^2(n) - \sin^2(n))
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>p(u)=\sqrt{\dfrac{3e^u}{u}}+2\sqrt{u}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  p'(u) \amp = \frac{1}{2\sqrt{\frac{3e^u}{u}}} \cdot \frac{d}{du} \left(\frac{3e^u}{u}\right) + 2\cdot \frac{1}{2\sqrt{u}} 
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{2\sqrt{\frac{3e^u}{u}}} \cdot \frac{3e^u \cdot u - 3e^u \cdot 1}{u^2} + \frac{1}{\sqrt{u}} 
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>q(w)=2e^{3\cos(w)}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  q'(w) \amp = 2e^{3\cos(w)} \cdot \frac{d}{dw} (3\cos(w)) 
							  </mrow>
							  <mrow>
								  \amp = 2e^{3\cos(w)} \cdot -3\sin(w)
							  </mrow>
							  <mrow>
								  \amp = -6e^{3\cos(w)}\sin(w)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			 
			  

		   <exercises>
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>y=\dfrac{1}{3}x^2 + 4x^{\frac{3}{2}} + \dfrac{3}{x} - \pi^e</m>
					  </p>
			  	
				  </statement>
			  
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  \frac{dy}{dx} \amp = \frac{1}{3} \frac{d}{dx}(x^2) + 4\frac{d}{dx}(x^{\frac{3}{2}}) + 3\frac{d}{dx}\left(\frac{1}{x}\right)
							  </mrow>
							  <mrow>
								  \amp = \frac{2}{3} x + 4 \cdot \frac{3}{2} x^{\frac{1}{2}} + 3\frac{d}{dx}(x^{-1})
							  </mrow>
							   <mrow>
								  \amp = \frac{2}{3} x + 4 \cdot \frac{3}{2} x^{\frac{1}{2}} - 3x^{-2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(t) = 3\sin (4t+\pi) + 4\cos(\pi - 5t^2)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(t) \amp = 3\cos(4t+\pi) \cdot \frac{d}{dt}(4t+\pi) - 4\sin(\pi-5t^2) \cdot \frac{d}{dt} (\pi-5t^2)
							  </mrow>
							  <mrow>
								  \amp = 3\cos(4t+\pi)\cdot 4 - 4\sin(\pi-5t^2) \cdot -10t^2
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>A(t) = 2e^{-1.5t}\sin(3t)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
					  	<md>
							  <mrow>
								  A'(t) \amp = 2\frac{d}{dt}(e^{-1.5t})\cdot \sin(3t) + 2e^{-1.5t} \cdot \frac{d}{dt}(\sin(3t))
							  </mrow>
							  <mrow>
								  \amp = 2e^{-1.5t} \cdot -1.5\sin(3t) + 2e^{-1.5t}\cdot \cos(3t)\cdot 3
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>p(y) = 2\ln(e^2+1) + 3\ln(y^2 +3y)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  p'(y) \amp = 0 + 3\cdot \frac{1}{y^2+3y} \cdot \frac{d}{dy}(y^2+3y)
							  </mrow>
							  <mrow>
								  \amp = \frac{3}{y^2+3y} \cdot (2y+3)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>r(x) = \arctan(4x\cos(x)) </m>
					  </p>
			  	
				  </statement>
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  r'(x) \amp = \frac{1}{1+ (4x\cos(x))^2}\cdot \frac{d}{dx}(4x\cos(x))
							  </mrow>
							  <mrow>
								  \amp =\frac{1}{1+ (4x\cos(x))^2}\cdot (\frac{d}{dx}(4x)\cdot \cos(x) + 4x\cdot \frac{d}{dx}(\cos(x)))
							  </mrow>
							  <mrow>
								  \amp =\frac{1}{1+ (4x\cos(x))^2}\cdot (4 \cos(x) - 4\sin(x))
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(t) = \dfrac{2t^4}{3e^t + 5t}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(t) \amp = \frac{\frac{d}{dt}(2t^4)\cdot(3e^t+5t) - 2t^4\cdot \frac{d}{dt}(3e^t + 5t)}{(3e^t+5t)^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{8t^3(3e^t+5t) - 2t^4 (3e^t + 5)}{(3e^t+5t)^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(z) = \ln(\cos(\sqrt{z}))</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(z) \amp = \frac{1}{\cos(\sqrt{z})} \cdot \frac{d}{dz}(\cos(\sqrt{z}))
							  </mrow>
							  <mrow>
								  \amp = \frac{1}{\cos(\sqrt{z})} \cdot -\sin(\sqrt{z})\cdot \frac{d}{dz}(\sqrt{z})
							  </mrow>
							  <mrow>
								  \amp = \frac{-\sin(\sqrt{z})}{\cos(\sqrt{z})} \cdot \frac{1}{2\sqrt{z}}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>q(x) = \dfrac{e^{4x}}{\tan(x^4)} </m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  q'(x) \amp = \frac{\frac{d}{dx}(e^{4x})\cdot \tan(x^4) - e^{4x} \cdot\frac{d}{dx}(\tan(x^4))}{\tan^2(x^4)}
							  </mrow>
							  <mrow>
								   \amp = \frac{e^{4x}\cdot 4\cdot \tan(x^4) - e^{4x} \cdot \sec^2(x^4)\cdot 4x^3}{\tan^2(x^4)}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			 
			  
		  </exercises>

		  <exercises>
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(x) = \dfrac{x^8}{2}-3\sqrt{x}+\dfrac{10}{e^2}</m>
					  </p>
			  	
				  </statement>
			  
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(x) \amp = \frac{1}{2}\frac{d}{dx}(x^8) - 3\frac{d}{dx}(\sqrt{x}) + \frac{d}{dx}\left(\frac{10}{e^2}\right)
							  </mrow>
							  <mrow>
								   \amp = \frac{1}{2} \cdot 8x^7 - 3\cdot \frac{1}{2\sqrt{x}} + 0
							  </mrow>
							  <mrow>
								   \amp = 4x^7 -  \frac{3}{2\sqrt{x}} 
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(y) =  2y-2\sqrt{\tan(y^2-2y)}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(y) \amp = 2 - 2\cdot\frac{1}{2\sqrt{\tan(y^2-2y)}}\cdot\frac{d}{dy}(\tan(y^2-2y))
							  </mrow>
							  <mrow>
								  \amp = 2 - \frac{1}{\sqrt{\tan(y^2-2y)}}\cdot\sec^2(y^2-2y)\cdot\frac{d}{dy}(y^2-2y)
							  </mrow>
							  <mrow>
								  \amp = 2 - \frac{1}{\sqrt{\tan(y^2-2y)}}\cdot\sec^2(y^2-2y)\cdot(2y-2)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>r(z)=\dfrac{-2\sin(2z+\pi)}{\tan(3z)+3z}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  r'(z) \amp = \frac{\frac{d}{dz}(-2\sin(2z+\pi))\cdot(\tan(3z)+3z) - (-2\sin(2z+\pi)) \cdot\frac{d}{dz}(\tan(3z)+3z)}{(\tan(3z)+3z)^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{-2\cos(2z+\pi)\cdot 2\cdot(\tan(3z)+3z) + 2\sin(2z+\pi) \cdot(\sec^2(3z)\cdot 3+3)}{(\tan(3z)+3z)^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{-4\cos(2z+\pi)(\tan(3z)+3z) + 2\sin(2z+\pi) \cdot(3\sec^2(3z)+3)}{(\tan(3z)+3z)^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(t)=-2e^{6\cos(t)}-\dfrac{2}{\sqrt{t}}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(t) \amp = -2e^{6\cos(t)}\cdot \frac{d}{dt}(6\cos(t)) - 2\frac{d}{dt}(t^{-1/2})
							  </mrow>
							  <mrow>
								  \amp = -2e^{6\cos(t)}\cdot-6\sin(t) - 2\cdot -\frac{1}{2}t^{-3/2}
							  </mrow>
							  <mrow>
								  \amp = 12\sin(t)e^{6\cos(t)} + t^{-3/2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>m(\theta)=3\sin(\theta)\cos(3\theta)</m>
					  </p>
			  	
				  </statement>
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  m'(\theta) \amp = \frac{d}{d\theta}(3\sin(\theta))\cdot\cos(3\theta) + 3\sin(\theta)\cdot\frac{d}{d\theta}(\cos(3\theta))
							  </mrow>
							  <mrow>
								  \amp = 3\cos(\theta)\cos(3\theta) + 3\sin(\theta)\cdot -\sin(3\theta)\cdot 3
							  </mrow>
							   <mrow>
								  \amp = 3\cos(\theta)\cos(3\theta) - 9\sin(\theta)\sin(3\theta)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>v(n)=\dfrac{2n+3}{3n^2+e^3}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  v'(n) \amp = \frac{\frac{d}{dn}(2n+3)\cdot(3n^2+e^3) - (2n+3)\cdot\frac{d}{dn}(3n^2+e^3)}{(3n^2+e^3)^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{2(3n^2+e^3) - (2n+3)\cdot(6n)}{(3n^2+e^3)^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>p(u)=\sin\left(\dfrac{2}{u}\right)-\cos\left(\dfrac{1}{2u}\right)+\ln\left(\dfrac{u}{2}\right)</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  p'(u) \amp = \frac{d}{du}(\sin(2u^{-1})) - \frac{d}{du}\left(\cos\left(\frac{1}{2}u^{-1}\right)\right)+\frac{d}{du}\left(\ln\left(\frac{1}{2}u\right)\right)
							  </mrow>
							  <mrow>
								  \amp = \cos(2u^{-1})\cdot (-2u^{-2}) - \left(-\sin\left(\frac{1}{2}u^{-1}\right)\right) \cdot -\frac{1}{2} u^{-2} + \frac{1}{\frac{1}{2}u} \cdot \frac{1}{2}
							  </mrow>
							  <mrow>
								  \amp = -\frac{2}{u^2}\cos\left(\frac{2}{u}\right) - \frac{1}{2u^2}\sin\left(\frac{1}{2u}\right) + \frac{1}{u}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>q(w)=e^{\, w\arctan(w)-2}-2</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  q'(w) \amp = e^{w\arctan(w)-2}\cdot \frac{d}{dw}(w\arctan(w)-2) - 0
							  </mrow>
							  <mrow>
								  \amp = e^{w\arctan(w)-2}\cdot \left(\frac{d}{dw}(w)\cdot\arctan(w) + w\cdot\frac{d}{dw}(\arctan(w))  \right)
							  </mrow>
							  <mrow>
								  \amp = e^{w\arctan(w)-2}\cdot \left(\arctan(w) + w\cdot\frac{1}{1+w^2}\right)
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			 
			  

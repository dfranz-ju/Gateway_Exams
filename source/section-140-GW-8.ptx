		   <exercises>
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>f(x) = 5x^4+\dfrac{1}{3x^8}-2\sqrt{x}</m>
					  </p>

				  </statement>
			  
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  f'(x) \amp = 5\frac{d}{dx}(x^4) + \frac{1}{3}\frac{d}{dx}(x^{-8}) - 2\frac{d}{dx}(\sqrt{x})
							  </mrow>
							  <mrow>
								   \amp = 5\cdot 4x^3 + \frac{1}{3}\cdot -8x^{-9} - 2\cdot\frac{1}{2\sqrt{x}}
							  </mrow>
							  <mrow>
								   \amp = 20x^3 - \frac{8}{3}x^{-9} - \frac{1}{\sqrt{x}}
							  </mrow>
						  </md>
					  </p>
				  

				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>g(y)=\cos(3y)-3\sin(y)+\dfrac{\tan(3y)}{3}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  g'(y) \amp = \frac{d}{dy}(\cos(3y)) - 3\frac{d}{dy}(\sin(y)) + \frac{1}{3}\frac{d}{dy}(\tan(3y))
							  </mrow>
							  <mrow>
								  \amp = -\sin(3y)\cdot 3 - 3 \cos(y) + \frac{1}{3} \sec^2(3y)\cdot 3
							  </mrow>
							  <mrow>
								  \amp = -3\sin(3y)- 3 \cos(y) + \sec^2(3y)
							  </mrow>
						  </md>
					  </p>

				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>r(z)=2e^z\ln(2z)</m>
					  </p>

				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  r'(z) \amp = \frac{d}{dz}(2e^z) \cdot \ln(2z) + 2e^z\cdot \frac{d}{dz}(\ln(2z))
							  </mrow>
							  <mrow>
								  \amp = 2e^z\ln(2z) + 2e^z\cdot \frac{1}{2z} \cdot 2
							  </mrow>
							  <mrow>
								  \amp = 2e^z\ln(2z) + \frac{2e^z}{z}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>h(t)=t^2+\arctan(\pi t^2)</m>
					  </p>

				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  h'(t) \amp = 2t + \frac{1}{1+(\pi t^2)^2} \cdot \frac{d}{dt}(\pi t^2)
							  </mrow>
							  <mrow>
								  \amp = 2t + \frac{1}{1+ (\pi t^2)^2} \cdot 2\pi t
							  </mrow>
							  <mrow>
								  \amp = 2t + \frac{2\pi t}{1+ (\pi t^2)^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m> m(\theta)=\dfrac{\theta^3+\tan(\theta)}{\theta-1}</m>
					  </p>

				  </statement>
			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  m'(\theta) \amp = \frac{\frac{d}{d\theta}(\theta^3+\tan(\theta)) \cdot(\theta-1) - (\theta^3+\tan(\theta)) \cdot \frac{d}{d\theta}(\theta -1)}{(\theta -1)^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{(3\theta^2+\sec^2(\theta)) \cdot(\theta-1) - (\theta^3+\tan(\theta))}{(\theta -1)^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m> v(n)=n-\cos\big(n\sin(n)\big)</m>
					  </p>
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  v'(n) \amp = 1 - \left(-\sin\big(n\sin(n)) \frac{d}{dn}(n\sin(n)) \right)
							  </mrow>
							  <mrow>
								  \amp = 1 + \sin\big(n\sin(n)\big) \cdot \left(\frac{d}{dn}(n) \cdot \sin(n) + n \cdot \frac{d}{dn}(\sin(n))\right)
							  </mrow>
							  <mrow>
								  \amp = 1 + \sin\big(n\sin(n)\big) \cdot (\sin(n) + n\cos(n))
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>p(u) = \dfrac{e^{2\sin(u)}}{\ln(u^2+1)}</m>
					  </p>

				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  p'(u) \amp = \frac{\frac{d}{du}(e^{2\sin(u)})\cdot\ln(u^2+1) - e^{2\sin(u)} \cdot \frac{d}{du}(\ln(u^2+1))}{(\ln(u^2+1))^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{e^{2\sin(u)}\cdot 2\cos(u)\cdot\ln(u^2+1) - e^{2\sin(u)} \cdot \frac{1}{u^2+1} \cdot 2u}{(\ln(u^2+1))^2}
							  </mrow>
							  <mrow>
								  \amp = \frac{2e^{2\sin(u)}\cos(u)\ln(u^2+1) - e^{2\sin(u)} \cdot \frac{2u}{u^2+1}}{(\ln(u^2+1))^2}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			  
			  
			  <exercise>
				  <statement>
					  <p>
						  <m>q(w)=\sqrt{\sin(\sqrt{w})}</m>
					  </p>
			  	
				  </statement>

			  
				  <solution>
					  <p>
						  <md>
							  <mrow>
								  q'(w) \amp = \frac{1}{2\sqrt{\sin(\sqrt{w})}} \cdot \frac{d}{dw}(\sin(\sqrt{w}))
							  </mrow>
							  <mrow>
								  q'(w) \amp = \frac{1}{2\sqrt{\sin(\sqrt{w})}} \cdot \cos(\sqrt{w})\cdot \frac{d}{dw}(\sqrt{w})
							  </mrow>
							  <mrow>
								  q'(w) \amp = \frac{1}{2\sqrt{\sin(\sqrt{w})}} \cdot \cos(\sqrt{w})\cdot \frac{1}{2\sqrt{w}}
							  </mrow>
						  </md>
					  </p>
				  
				  </solution>
			  </exercise>
			 
			  
